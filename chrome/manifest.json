{
  "name": "Sweetube",
  "version": "1.0",
  "description": "Sweetest YouTube Ever",
  "permissions": [
    "webNavigation",
    "tabs",
    "http://localhost/*"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["contentScript.js"],
      "matches": ["*://*.youtube.com/*"]
    }
  ],
  "manifest_version": 2
}
